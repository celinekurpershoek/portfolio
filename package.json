{
  "name": "celinekurpershoek",
  "version": "1.0.0",
  "description": "Portfolio website for Celine Kurpershoek",
  "scripts": {
    "prebuild": "rimraf dist/ && mkdirp dist/",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:assets": "ncp src/assets/ dist/assets/",
    "build:html": "ncp src/index.html dist/index.html",
    "prestart": "npm run build",
    "start": "npm run start:prod",
    "start:prod": "http-server './dist/' -c-1 -o -p 2810",
    "watch": "chokidar 'src/*.html' -c 'npm run build'"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.1.3",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celinekurpershoek/portfolio.git"
  },
  "keywords": [],
  "author": "Celine Kurpershoek",
  "license": "UNLICENSED"
}
